    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/ssr": "^20.0.3",
    "@longhl104/pawfect-match-ng": "^0.2.0",
    "@primeuix/themes": "^1.2.3",
    "express": "^5.1.0",
