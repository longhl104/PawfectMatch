{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl true --ssl-cert ../../Shared/local-certs/localhost.pem --ssl-key ../../Shared/local-certs/localhost-key.pem",
    "start:prod": "ng serve --configuration production",
    "build": "ng build",
    "build:development": "ng build --configuration development",
    "build:production": "ng build --configuration production",
    "build:staging": "ng build --configuration staging",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:client": "node dist/client/server/server.mjs",
    "lint": "ng lint",
    "nswag": "nswag openapi2tsclient /Input:https://localhost:7000/swagger/v1/swagger.json /output:./src/app/shared/apis/generated-apis.ts /template:Angular /injectionTokenType:InjectionToken /className:{controller}Api /operationGenerationMode:MultipleClientsFromFirstTagAndOperationId /typeScriptVersion:5.8 /rxJsVersion:7.8 /dateTimeType:String /EnumStyle:Enum /GenerateOptionalParameters:true"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
