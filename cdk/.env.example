# AWS CDK Environment Configuration Template
# Copy this file to .env and replace with your actual values

# Your AWS Account ID (12-digit number)
CDK_DEFAULT_ACCOUNT=your-account-id-here
CDK_PRODUCTION_ACCOUNT=your-production-account-id-here

# Your preferred AWS Region (Australian regions: ap-southeast-2, ap-southeast-4)
CDK_DEFAULT_REGION=ap-southeast-2

# Optional: Additional environment variables
CDK_STAGE=development
